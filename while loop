i = 1
while i < 6:
  print(i)
  i += 1
else:
  print("code is done ")
#decremental while loop
i = 30
while i > 0:
    print(i)
    i = i-1
else:
    print("code is done")

# using input funtions 

i = int(input("num"))
while i<20:
    print(i)
    i = i+1

# one more example 

i = int(input("num"))
x = int(input('enter numbr greater then i :'))
while i<x:
    print(i)
    i = i+1
